{
  "name": "reactor2.0",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "connected-react-router": "^4.5.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "forcedomain": "^1.0.0",
    "ignore-styles": "^5.0.1",
    "js-cookie": "^2.2.0",
    "lint-staged": "^8.1.0",
    "md5-file": "^4.0.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.11.0",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-frontload": "^1.0.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start-app": "react-scripts start",
    "start": "set REACT_APP_ENV=local&& npm run start-app",
    "start:local": "set REACT_APP_ENV=local&& npm run start-app",
    "start:stag": "set REACT_APP_ENV=stag&& npm run start-app",
    "start:prod": "set REACT_APP_ENV=prod&& npm run start-app",
    "https": "set HTTPS=true && npm start",
    "build-app": "react-scripts build",
    "build:local": "set REACT_APP_ENV=local&& npm run build-app",
    "build": "set REACT_APP_ENV=local&& npm run build-app",
    "build:stag": "set REACT_APP_ENV=stag&& npm run build-app",
    "build:prod": "set REACT_APP_ENV=prod&& npm run build-app",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "cross-env NODE_ENV=production node ./server/index.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.7.0"
  }
}
